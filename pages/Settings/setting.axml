<view class="setting-container">
  <view class="profile-container">
    <view class="sub-container">
      <image
        src="data:image/jpeg;base64,660d309a6ded3e06f324686f"
        mode="aspectFit"
        class="profilepicture"
        id="profile-image"
      />

    </view>
    <view class="camera-btn" onTap="openprofilepicturechange">
      <image class="bg-camera" mode="scaleToFill" src="../../Assets/camera_bg.png" />
      <image class="camera" mode="scaleToFill" src="../../Assets/camera.png" />
    </view>
    <view class="profil-text">{{userProfile.firstName}}</view>
    <view class="profil-text">{{userProfile.phoneNumber}}</view>
  </view>
  <view class="title-name-profile " style="margin-left:30px!important"> Settings {{greeting}}
  </view>
  <view class="setting-btn">
    <button size="default" type="primary" onTap="showPin">
      <image mode="scaleToFill" src="../../Assets/lock-icon.png" />Change Pin
    </button>
    <button size="default" onTap="showChangePhonenumber" type="primary">
      <image mode="scaleToFill" src="../../Assets/call-icon.png" />Change Phone number
    </button>
    <button size="default" onTap="termsandconditionBtn" type="primary">
      <image mode="scaleToFill" src="../../Assets/term-condition.png" />{{ terms_condition}}
    </button>
    <button size="default" type="primary" onTap="privacyandpolicybtn">
      <image mode="scaleToFill" src="../../Assets/term-condition.png" />Privacy and Policy
    </button>
    <button size="default" type="primary" onTap="aboutinfoBtn">
      <image mode="scaleToFill" src="../../Assets/about-50.png" />About Shukarani
    </button>
    <button size="default" type="primary" onTap="faqBtn">
      <image id="faq" mode="scaleToFill" src="../../Assets/faq-48.png" />FAQs
    </button>

  </view >
  <view class="language-name-profile2">Change language ?</view>
  <view  class="language" id="ksw" onTap="changelanguagebtn"> {{ currentLanguage === "es" ? "KSW" : "ENG" }}</view>
  <!-- <view show={{}} class="language" id="eng" onTap="changelanguagebtn">Eng</view> -->
  <view class="language-name-profile" onTap="opendelect">Delect my account</view>
  <view class="language-name-profile3">About Shukarani V.2</view>
  <button size="default" class="sigin-out-btn" type="primary">
    Sign Out
    <image mode="scaleToFill" src="../../Assets/btn_arrow_right.png" />
  </button>
  <!-- CHANGE PIN SECTION  -->
  <model show="{{changePin}}" position="bottom" closePopup="closewithdraw">
    <view class="close-btn" onTap="closewithdraw">
      <icon class="close-icon" type="clear" aria-label="clear" />
    </view>
    <view style="margin:3%" class="changepin">
      <image mode="scaleToFill" src="../../Assets/icons-lock.png" class="changePinImg" />
      <view class="changePinTitle">Change Pin</view>
      <view class="changeBtnSection">
        <view>
          <f-input placeholder="Enter Pin" class="p-20" onInput="enterPin">
            <image src="/Assets/lock-icon.png" slot="iconInnerLeft" />
          </f-input>
        </view>
        <view>
          <f-input placeholder="Re-Enter Pin" class="p-20" onInput="reenterPin">
            <image src="/Assets/lock-icon.png" slot="iconInnerLeft" />
          </f-input>
        </view>

        <button size="default" type="primary" onTap="chaangePin" class="nextBtn">
          Next
          <image
            mode="scaleToFill"
            src="../../Assets/btn_arrow_right.png"
            class="changePinImg"
          />
        </button>
      </view>
    </view>
  </model>
  <!--<model show="{{changePinOTP}}" position="bottom" closePopup="closewithdraw"><view class="close-btn" onTap="closewithdrawOTP"><icon class="close-icon" type="clear" aria-label="clear" /></view><view style="margin:3%" class="changepin"><image mode="scaleToFill" src="../../Assets/icons-lock.png" class="changePinImg" /><view class="changePinTitle">Enter OTP</view><view class="changeBtnSection"><view class="changePinnumber">Enter OTP sent to *****34</view><view class="OTPenterbox"><f-input placeholder="Enter the OTP" class="p-20" onInput="enterOTP"><image src="/Assets/lock-icon.png" slot="iconInnerLeft" /></f-input></view><button size="default" type="primary" onTap="SuccessNext" class="nextBtn">
          Next<image
            mode="scaleToFill"
            src="../../Assets/btn_arrow_right.png"
            class="changePinImg"
          /></button></view></view></model> -->
  <!-- CHANGE PASSWORD SECTION -->
  <model show="{{ShowchangePNumber}}" position="bottom" closePopup="closewithdraw">
    <view class="close-btn" onTap="closePhoneNumberchange">
      <icon class="close-icon" type="clear" aria-label="clear" />
    </view>
    <view style="margin:3%" class="changepin">
      <image mode="scaleToFill" src="../../Assets/b_call.png" class="changePinImg" />
      <view class="changePinTitle">Change Phone Number</view>

      <view class="changeBtnSection">
        <view class="changePinnumber">
          Change ***** {{(userProfile.phoneNumber).slice(-3)}} as your number
        </view>
        <view class="reward-input pay-number-input">
          <input placeholder="New Phone Number" onInput="changenumber" />
        </view>
        <button size="default" type="primary" onTap="changePhonenumber" class="phoneNumberchange">
          Change
        </button>
      </view>
    </view>
  </model>
  <model show="{{ShowchangePNumberOTP}}" position="bottom" closePopup="closeChangeNumberOTP">
    <view class="close-btn" onTap="closeChangeNumberOTP">
      <icon class="close-icon" type="clear" aria-label="clear" />
    </view>
    <view style="margin:3%" class="changepin">
      <image mode="scaleToFill" src="../../Assets/icons-lock.png" class="changePinImg" />
      <view class="changePinTitle">Authentication</view>

      <view class="changeBtnSection">
        <view class="changePinnumberOTP">
          {{ enter_otp }} sent to ***** {{(userProfile.phoneNumber).slice(-3)}}
        </view>
        <f-input placeholder={{enter_otp}} class="p-10 " onInput="changenumbrtOTP">
          <image src="/Assets/lock-icon.png" slot="iconInnerLeft" />
        </f-input>
        <button size="default" type="primary" onTap="OTPvalidate" class="nextBtn">
          Save
          <image
            mode="scaleToFill"
            src="../../Assets/btn_arrow_right.png"
            class="changePinImgs"
          />
        </button>
      </view>
    </view>
  </model>
  <!-- CHANGE PROFILE PICTURE SECTION  -->
  <model
    show="{{Showchangeprofilepicture}}"
    position="bottom"
    closePopup="closewithdraw"
    id="changeprofilesection"
  >
    <view class="close-btn" onTap="closeprofilechange">
      <icon class="close-icon" type="clear" aria-label="clear" />
    </view>
    <view style="margin:3%" class="changepin">

      <view class="changeProfilepicTitle">Change Profile Picture</view>

      <view class="changeprofileBtnSection">
        <button size="default" type="primary" onTap="choosephoto" class="profileChoosebtn">
          Choose Photo
        </button>

        <button
          size="default"
          type="primary"
          onTap="openChangenumberOTP"
          class="profileChoosebtn"
        >
          Take Photo
        </button>
      </view>
    </view>
  </model>
